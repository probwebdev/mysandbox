{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "export": {
      "dependsOn": ["^export"],
      "outputs": [".next/**", "out/**"]
    },
    "test:unit:coverage": {
      "inputs": [
        "./src/**/__tests__/**/*.test.ts",
        "./src/**/__tests__/**/*.test.tsx",
        "./src/**/__tests__/../*.ts",
        "./src/**/__tests__/../*.tsx",
        "./**/mocks/**/*js",
        "./jest.config.js",
        "./config/jest.setup.js"
      ],
      "outputs": ["coverage/**"]
    },
    "lint:js": {
      "inputs": ["./**/*.js", "./**/*.ts", "./**/*.tsx"],
      "outputs": []
    },
    "lint:css": {
      "inputs": ["./src/**/*.css"],
      "outputs": []
    },
    "lint:format": {
      "inputs": [
        "./**/*.js",
        "./**/*.tsx",
        "./**/*.md",
        "./**/*.css",
        "./**/*.json"
      ],
      "outputs": []
    }
  }
}
